{
  "name": "sax-wasm",
  "version": "2.2.4",
  "repository": "https://github.com/justinwilaby/sax-wasm",
  "description": "An extremely fast JSX, HTML and XML parser written in Rust compiled to WebAssembly for Node and the Web",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/esm/index.js",
  "files": [
    "lib/**"
  ],
  "scripts": {
    "test": "jest",
    "test:coveralls": "npm run test && nyc report --reporter=text-lcov | coveralls",
    "build:wasm": "rustc ./src/lib.rs -Clto -O --crate-type cdylib --target wasm32-unknown-unknown -o ./lib/sax-wasm.wasm",
    "bindgen": "wasm-bindgen lib/sax-wasm.wasm --out-dir lib/ --remove-name-section --no-typescript --out-name sax-wasm.wasm",
    "build": "(npm run build:esm & npm run build:cjs) && npm run build:wasm && npm run bindgen && tsc --emitDeclarationOnly",
    "build:esm":"tsc --module esnext --outDir lib/esm --target esnext",
    "build:cjs":"tsc --module commonjs --outDir lib/cjs --target esnext",
    "benchmark": "node src/js/__test__/benchmark.js",
    "lint": "eslint src/js/ --ext .ts"
  },
  "keywords": [
    "XML",
    "parser",
    "sax",
    "web",
    "assembly"
  ],
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "types":"./lib/index.d.ts",
  "author": "Justin Wilaby",
  "license": "MIT",
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.2",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "eslint": "9.17.0",
    "expect.js": "^0.3.1",
    "jest": "29.7.0",
    "libxmljs": "1.0.11",
    "ltx": "^3.0.0",
    "node-xml": "^1.0.2",
    "rollup": "4.29.1",
    "sax": "^1.2.4",
    "ts-jest": "29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
